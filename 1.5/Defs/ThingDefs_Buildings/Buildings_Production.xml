<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--============================== BLUE RIMBOOK STATION ==============================-->
  <ThingDef ParentName="BuildingBase">
    <defName>TableBlue</defName>
    <label>RimBook Content Station</label>
    <thingClass>Building_WorkTable</thingClass>
    <description>Produce and post Rimbook content to get followers.</description>
    <graphicData>
      <texPath>TableBlue</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3,1.2)</drawSize>
    </graphicData>
    <costList>
      <Steel>120</Steel>
      <ComponentIndustrial>8</ComponentIndustrial>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <minifiedDef>MinifiedThing</minifiedDef>
    <fillPercent>0.4</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <Mass>34</Mass>
      <WorkToBuild>1000</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <thingCategories>
      <li>BuildingsProduction</li>
    </thingCategories>
    <size>(3,1)</size>
    <designatorDropdown>RimApps_RimBook</designatorDropdown>
    <designationCategory>Production</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <recipes>
      <li>CraftBlue</li>
    </recipes>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
    <comps>
      <li Class="CompProperties_AffectedByFacilities">
        <compClass>CompAffectedByFacilities</compClass>
        <linkableFacilities>
          <li>ToolCabinet</li>
        </linkableFacilities>
      </li>
    </comps>
    <researchPrerequisites>
      <li>Content</li>
    </researchPrerequisites>
  </ThingDef>
